@model WorkShop
@{
    ViewData["Title"] = "Add";
}

<form id="WorkShopContainer">
    <div class="mb-3">
        <label class="form-label" asp-for="@Model.Title">Title</label>
        <input type="text" class="form-control" asp-for="@Model.Title">
    </div>   
    <div class="mb-3">
        <label asp-for="@Model.Description" class="form-label">Description</label>
        <input type="text" asp-for="@Model.Description" class="form-control">
    </div>
    <button type="submit" class="btn btn-primary" onclick="UpdateWorkShop(@Model.Id)">Submit</button>
</form>
<button onclick="Back('/WorkShop/GetAll')" class="btn btn-primary">Back</button>